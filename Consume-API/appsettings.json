{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ApplicationParameters": {
    "UsersApiEndpoint": "https://jsonplaceholder.typicode.com/users/",
    "TodosApiEndpoint": "https://jsonplaceholder.typicode.com/todos/"
  },
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200",
    "AutoRegisterTemplate": true,
    "IndexFormat": "poc-local-",
    "User": "elastic",
    "Pwd": "y0Voo81G5W0RLzWls9ZY",
    "NumberOfShards": 2,
    "NumberOfReplicas": 1,
    "EmitEventFailure": "WriteToSelfLog",
    "MinimumLogEventLevel": "Information"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Overrides": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifeime": "Warning"
      }
    },
    "Usings": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact",
          "path": "./Logs/log.json",
          "rollingInterval": "Day"
        }
      }
    ]
  }
  //Can be configured
  //"HealthChecksUI": {
  //  "HealthChecks": [
  //    {
  //      "Name": "",
  //      "Uri": ""
  //    }
  //  ],
  //  "EvaluationTimeInSeconds": 10
  //}
}
